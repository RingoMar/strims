{
  "name": "memes",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "pb:types": "nodemon --watch ./schema -e proto --delay 0.1 --signal SIGINT --exec './schema/build.sh || exit 1'",
    "pb:register": "nodemon --watch ./schema -e proto --delay 0.1 --signal SIGINT --exec './ts-codegen.sh || exit 1'",
    "app": "electron ./dist/app/main.js",
    "app:webpack": "webpack --mode development --watch",
    "app:go": "nodemon --watch ./pkg --watch ./cmd/app -e go --delay 0.1 --signal SIGINT --exec 'go build -o ./dist/app/p2p ./cmd/app || exit 1'",
    "dev:app": "npx run-p pb:* app:webpack app:go app",
    "web": "webpack-dev-server --mode development --watch",
    "web:go": "nodemon --watch ./pkg -e go --exec 'touch src/web/p2p.go'",
    "dev:web": "npx run-p pb:* web:go web",
    "build": "webpack --mode production"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.7.2",
    "@types/react-router-dom": "^5.1.3",
    "autoprefixer": "^9.7.1",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "electron": "^7.1.2",
    "farce": "^0.2.8",
    "file-loader": "^5.0.2",
    "golang-wasm-async-loader": "file:./vendor/webpack-golang-wasm-async-loader",
    "google-protobuf": "^3.11.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "mux.js": "^5.5.1",
    "node-sass": "^4.13.0",
    "nodemon": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "react": "^16.12.0",
    "react-async": "^9.0.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.1",
    "react-router-dom": "^5.1.2",
    "react-use": "^13.2.1",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.2.1",
    "ts-loader": "^6.2.1",
    "ts-protoc-gen": "^0.12.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2",
    "typescript-tslint-plugin": "^0.5.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "browserslist": [
    "defaults"
  ]
}
