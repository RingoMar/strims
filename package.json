{
  "name": "memes",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "pb:gen": "nodemon --watch ./schema -e proto --delay 0.1 --signal SIGINT --exec './hack/pb-codegen.sh || exit 1'",
    "app:electron": "electron ./dist/app/main.js",
    "app:webpack": "webpack --mode development --watch",
    "app:go": "nodemon --watch ./pkg --watch ./cmd/app -e go --delay 0.1 --signal SIGINT --exec 'go build -o ./dist/app/p2p ./cmd/app || exit 1'",
    "dev:app": "run-p pb:* app:webpack app:go app:electron",
    "web:webpack": "webpack-dev-server --mode development --watch",
    "web:go": "nodemon --watch ./pkg -e go --exec 'touch src/web/svc.go'",
    "web:test:integration": "nodemon --watch ./pkg --watch ./integration --watch ./src -e go,js,jsx,ts,tsx --delay 0.1 --signal SIGINT --exec 'go test -mod=readonly ./integration/frontend -tags web -count 1 -v 1 || exit 1'",
    "dev:web": "run-p pb:* web:go web:webpack",
    "test:web": "run-p dev:web web:test:integration",
    "svc": "nodemon --watch ./pkg --watch ./cmd/svc -e go --delay 0.1 --signal SIGINT --exec 'go run -mod=readonly ./cmd/svc || exit 1'",
    "test:native": "nodemon --watch ./pkg --watch ./integration -e go --delay 0.1 --signal SIGINT --exec 'go test -mod=readonly ./integration/frontend -tags native -count 1 -v 1 || exit 1'",
    "dev:svc": "run-p pb:* svc",
    "build": "webpack --mode production"
  },
  "devDependencies": {
    "@types/dom-mediacapture-record": "^1.0.6",
    "@types/google-protobuf": "^3.7.2",
    "@types/react-document-title": "^2.0.4",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-select": "^3.0.13",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/eslint-plugin-tslint": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "autoprefixer": "^9.7.6",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^3.0.0",
    "clsx": "^1.1.0",
    "css-loader": "^3.5.3",
    "electron": "^7.3.0",
    "electron-json-storage": "^4.1.8",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsdoc": "^25.4.2",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "farce": "^0.2.8",
    "file-loader": "^5.1.0",
    "golang-wasm-async-loader": "file:./vendor/webpack-golang-wasm-async-loader",
    "google-protobuf": "^3.12.0",
    "html-webpack-plugin": "^3.2.0",
    "import-sort-style-module": "^6.0.0",
    "mediastream-to-webm": "^1.0.2",
    "mini-css-extract-plugin": "^0.8.2",
    "mux.js": "^5.6.2",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "prettier-plugin-import-sort": "0.0.4",
    "protobufjs": "git://github.com/protobufjs/protobuf.js.git#ceb73356d99a8747c08cb7051f3b3551ae8a3a0e",
    "protoc-gen-ts": "0.0.5",
    "react": "^16.13.1",
    "react-async": "^9.0.0",
    "react-async-hook": "^3.6.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-hook-form": "^4.10.2",
    "react-icons": "^3.10.0",
    "react-router-dom": "^5.2.0",
    "react-select": "^3.1.0",
    "react-use": "^13.27.1",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^2.3.6",
    "ts-ebml": "^2.0.2",
    "ts-loader": "^6.2.2",
    "ts-protoc-gen": "^0.12.0",
    "tslint": "^6.1.2",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.9.3",
    "typescript-tslint-plugin": "^0.5.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "worker-loader": "^2.0.0"
  },
  "browserslist": [
    "defaults"
  ],
  "importSort": {
