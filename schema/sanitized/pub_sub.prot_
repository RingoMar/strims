syntax = "proto3";

option go_package = "github.com/MemeLabs/go-ppspp/pkg/pb;pb";
option java_package = "gg.strims.ppspp.proto";
option swift_prefix = "PB";

message PubSubEvent {
  message Message {
    int64 time = 1;
    string key = 3;
    bytes body = 4;
  }

  message Close {}

  message Padding {
    bytes body = 1;
  }

  oneof body {
    Message message = 1;
    Close close = 2;
    Padding padding = 3;
  }
}

